{
  "name": "angular-to-electron",
  "version": "1.0.0",
  "description": "CLI tool to convert Angular applications into Electron desktop applications",
  "main": "dist/index.js",
  "bin": {
    "angular-to-electron": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "prepare": "npm run build",
    "postinstall": "npm i --save-dev @types/node"
  },
  "keywords": [
    "angular",
    "electron",
    "cli",
    "desktop",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/yargs": "^17.0.32",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
} 